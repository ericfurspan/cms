{
  "defaultConnection": "default",
  "connections": {
    "default": {
      "connector": "mongoose",
      "settings": {
        "uri": "${process.env.MONGODB_URI || ''}",
        "database": "${process.env.DATABASE_NAME || 'strapi-db-dev'}"
      },
      "options": {
        "ssl": true
      }
    }
  }
}